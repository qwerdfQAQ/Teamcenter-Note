    "getConfirmationForDeleteCalendar": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "cal.deleteCalendar",
            "condition": ""
          }
        ]
      },
      "actionMessages": {
        "success": [
          {
            "condition": "ctx.mselected.length==1",
            "message": "deleteCalendarSingle"
