    "performRemoveOperation": {
      "actionType": "TcSoaService",
      "serviceName": "Administration-2017-05-RoleManagement",
      "method": "removeRolesFromGroup",
      "inputData": {
        "roleGroupStructs": [
          {
            "roles": "{{ctx.mselected}}",
            "grp": "{{ctx.pselected}}"
          }
        ]
      },
      "events": {
        "success": [
          {
            "name": "cdm.relatedModified",
            "eventData": {
              "refreshLocationFlag": false,
              "relations": "",
              "relatedModified": [
                "{{ctx.pselected}}"
              ]
            }
          },
          {
            "name": "OrgTree.reloadOrgTree",
            "condition": "conditions.isTreeMode",
            "eventData": {
              "isRemoved": true
            }
          }
        ],
        "failure": [
          {
            "name": "cdm.relatedModified",
            "eventData": {
              "refreshLocationFlag": false,
              "relations": "",
              "relatedModified": [
                "{{ctx.pselected}}"
              ]
            }
          }
        ]
      },
      "actionMessages": {
        "failure": [
          {
            "message": "removeOperationFailure"
          }
        ]
      }
    },
    "performRemoveOperationForUserObjects": {
      "actionType": "TcSoaService",
      "serviceName": "Administration-2017-05-UserManagement",
      "method": "removeGroupMembers",
      "inputData": {
        "userRoleGroupStructs": [
          {
            "users": "{{ctx.mselected}}",
            "grp": {
              "uid": "{{commandContext.searchState.criteria.groupUID}}",
              "type": "Group"
            },
            "role": "{{ctx.pselected}}"
          }
        ]
      },
      "events": {
        "success": [
          {
            "name": "cdm.relatedModified",
            "eventData": {
              "refreshLocationFlag": false,
              "relatedModified": [
                "{{ctx.pselected}}"
              ]
            }
          },
          {
            "name": "OrgTree.reloadOrgTree",
            "condition": "conditions.isTreeMode",
            "eventData": {
              "isRemoved": true
            }
          }
        ],
        "failure": [
          {
            "name": "cdm.relatedModified",
            "eventData": {
              "refreshLocationFlag": false,
              "relations": "",
              "relatedModified": [
                "{{ctx.pselected}}"
              ]
            }
          }
        ]
      },
      "actionMessages": {
        "failure": [
          {
            "message": "removeOperationFailure"
          }
        ]
      }
    },
