  "conditions": {
    "enableGroupCommandForTreeMode": {
      "expression": {
        "$and": [
          {
            "$source": "ctx.ViewModeContext",
            "$query": {
              "$source": "ViewModeContext",
              "$query": {
                "$in": [
                  "TreeView",
                  "TreeSummaryView"
                ]
              }
            }
          },
          {
            "$or": [
              {
                "$source": "commandContext.selectionData.selected",
                "$query": {
                  "$source": "levelNdx",
                  "$query": {
                    "$eq": 0
                  }
                }
              },
              {
                "$source": "commandContext.selectionData.selected",
                "$query": {
                  "$source": "type",
                  "$query": {
                    "$eq": "Group"
                  }
                }
              },
              {
                "$source": "commandContext.selectionData.selected.length",
                "$query": {
                  "$eq": 0
                }
              }
            ]
          }
        ]
      }
    },
    "isPeopleLocation": {
      "expression": "ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization' && commandContext.objectSetSource !== 'roles.Role' && commandContext.objectSetSource !== 'um0AdminDataProvider.User' && commandContext.objectSetSource !== 'um0AdminDataProvider.GroupMember'"
    },
    "isPeopleLocationTree": {
      "expression": "ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' &&  ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization' && ctx.ViewModeContext && (ctx.ViewModeContext.ViewModeContext === 'TreeView' || ctx.ViewModeContext.ViewModeContext === 'TreeSummaryView')"
    },
    "isPeopleLocationList": {
      "expression": "ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' &&  ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization' && ctx.ViewModeContext && ctx.ViewModeContext.ViewModeContext !== 'TreeView' && ctx.ViewModeContext.ViewModeContext !== 'TreeSummaryView'"
    },
    "isTreeMode": {
      "expression": "ctx.ViewModeContext && (ctx.ViewModeContext.ViewModeContext === 'TreeView' || ctx.ViewModeContext.ViewModeContext === 'TreeSummaryView')"
    },
    "enableShowAddObjectsGroupCommandForTreeView": {
      "expression": "ctx.userSession!==null && conditions.enableGroupCommandForTreeMode"
    },
    "enableShowAddObjectsGroupCommandForListView": {
      "expression": "ctx.userSession!==null && !conditions.isTreeMode && !(ctx.state.processed.d_uids && ctx.state.processed.d_uids.indexOf('^') > -1) && ( !ctx.selected  || ctx.visibleServerCommands.Um0AddInAnOrganization)"
    },
    "enableChangePassword": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.selected.type === 'User' && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation'"
    },
    "enableCreateGroup": {
      "expression": "ctx.userSession!==null && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'groups'"
    },
    "enableCreateRole": {
      "expression": "ctx.userSession!==null && !ctx.selected && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'roles'"
    },
    "enableCreateUser": {
      "expression": "ctx.userSession!==null && !ctx.selected && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'users'"
    },
    "enableAddInAnOrganization": {
      "expression": "!commandContext.searchState.criteria.roleUID && ctx.userSession!==null && ( !ctx.selected || (ctx.selected && ( commandContext.selectionData.selected[0].type === 'Site' || commandContext.selectionData.selected[0].type === 'Group' ) && commandContext.selectionData.selected.length === 1)) && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization' "
    },
    "enableShowAddObjectsRole": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.selected.type === 'Group' && ( ((commandContext.selectionData.selected.length === 1 || commandContext.vmo ) && conditions.isTreeMode ) || !conditions.isTreeMode) && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization'"
    },
    "enableShowAddObjectsUser": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.selected.type === 'Role' && ( ((commandContext.selectionData.selected.length === 1 || commandContext.vmo ) && conditions.isTreeMode) || !conditions.isTreeMode) && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization'"
    },
    "enableShowAddObjectsRolesInGroupTab": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.selected.type === 'Group' && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'groups'"
    },
    "enableRemoveAdminObjectsRole": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.selected.type === 'Role' && ctx.pselected.type === 'Group' && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization'"
    },
    "enableRemoveAdminObjectsUser": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.selected.type === 'User' && ctx.pselected.type === 'Role' && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization'"
    },
    "enableRemoveAdminObjectsRolesInGroupTab": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.selected.type === 'Role' && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'groups'"
    },
    "enableRemoveAdminObjectsGroupMember": {
      "expression": "ctx.userSession!==null && ctx.selected && ctx.pselected.type === 'User' && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization'"
    },
    "enableShowAddObjectsInOrg": {
      "expression": "ctx.userSession!==null && ctx.selected && (ctx.selected.type === 'Group' || ctx.selected.type === 'Role' || ctx.selected.type === 'User' ) && ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'organization'"
    },
    "enableNavigateObjects": {
      "expression": "(commandContext.vmo.type === 'Group' || commandContext.vmo.type === 'Role')"
    },
    "isBaseCalendarSublocation": {
      "expression": "ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'baseCalendars' && ctx.userSession.props.group_name.dbValue === 'dba'"
    },
    "isAwDeleteCalendarCmdEnabled": {
      "expression": "ctx.mselected[0].type === 'TCCalendar'"
    },
    "isAwAddCalendarCmdEnabled": {
      "expression": "!ctx.selected"
    },
    "deleteUserCalendarCommandVisibility": {
      "expression": "( ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'users' && ctx.userCalendar && ( (ctx.userSession.props.group_name.dbValue === 'dba') || ( ctx.mselected[0].uid === ctx.user.uid ) ) )"
    },
    "isCalendarLocation": {
      "expression": "commandContext.pageContext.primaryActiveTabId === 'tc_xrt_UserCalendar' || commandContext.pageContext.secondaryActiveTabId === 'tc_xrt_UserCalendar' || (ctx.locationContext['ActiveWorkspace:Location'] === 'com.siemens.splm.client.usermanagement.peoplelocation' && ctx.locationContext['ActiveWorkspace:SubLocation'] === 'baseCalendars')"
    }
  },
