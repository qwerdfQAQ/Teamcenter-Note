                        "startReached":"{{data.dataProviders.gridDataProvider.cursorObject.startReached}}",
                        "endReached":"{{data.dataProviders.gridDataProvider.cursorObject.endReached}}"
                    },
                    "focusObjUid": "{{ctx.newlyCreatedObj.uid}}",
