            "action": "startEditForNewVmos"
        },
        {
            "eventId": "saveEditsRequested",
            "action": "invokeSaveEditsCommand"
        },
        {
            "eventId": "cdm.relatedModified",
            "cacheEventData": true,
            "action": "handleCdmRelatedModifiedEvent",
            "inputArgs": {
