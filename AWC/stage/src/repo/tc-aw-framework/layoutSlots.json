{
  "slots": {
    "awAppLogoSlot": {
      "name": "aw_appLogo",
      "view": "AwAppLogo",
      "activeWhen": true
    },
    "performanceMonitoringPanelVisSlot": {
      "name": "aw_plstats_widgets_slot",
      "view": "Awv0PLStatsPanel",
      "activeWhen": true
    },
    "loggingPanelVisSlot": {
      "name": "otherModuleLoggingViewSlot",
